{
  "name": "sip-smart-scan",
  "version": "1.0.0",
  "description": "A complete mobile application for drink analysis with AI-powered nutrition insights and health recommendations.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "expo start",
    "start:backend": "cd backend && python start.py",
    "start:frontend-only": "expo start",
    "start:backend-only": "cd backend && python start.py",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "install:all": "npm install && npm run install:python",
    "install:python": "pip install -e .",
    "test": "jest",
    "lint": "eslint .",
    "type-check": "tsc",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.16",
    "@tanstack/react-query": "^5.17.19",
    "axios": "^1.6.7",
    "expo": "~52.0.0",
    "expo-camera": "~16.0.18",
    "expo-device": "~7.0.3",
    "expo-file-system": "~18.0.12",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-notifications": "~0.29.14",
    "expo-status-bar": "~2.0.1",
    "react": "18.3.1",
    "react-hook-form": "^7.49.3",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
